<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%=title%></title>
</head>
<body>
   
   <% if(users!=""){ %>
    <table>
        <thead>
            <tr>
                <th>Sno</th>
                <th>Name</th>
                <th>Email</th>
                <th>Password</th>
                <th>Action</th>
            </tr>
            <tbody>
                <% users.forEach((user,index)=>{ %>
                    <tr>
                        <td><%=index+1%></td>
                        <td><%=user.name%></td>
                        <td><%=user.email%></td>
                        <td><%=user.password%></td>
                        <td>
                            <a href="/edit/<%=user._id%>">Update</a>
                            <a href="/delete/<%=user._id%>">Delete</a>
                        </td>
                    </tr>

                    <%})%>
                
            </tbody>
        </thead>
    </table>
   

   <% } else {%>
<h1>No user Found</h1>
   <% }%>
</body>
</html>